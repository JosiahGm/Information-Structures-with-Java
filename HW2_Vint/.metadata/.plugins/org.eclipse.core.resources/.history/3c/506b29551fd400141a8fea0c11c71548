package cs520.hw2.pt2;

public class Test {
	
	public static void main(String[] args){
		
		//Declare new course object with appropriate values
		Course currentCourse = new Course("BIO 140", 1100, 4);
		
		//print course info with toString method
		System.out.println("\n" + currentCourse.toString());
		
		//redefine object as OnlineCourse with appropriate values
		currentCourse = new OnlineCourse("CS 520", 1100, 3, 400);
		
		//print online course info with toString method
		System.out.println("\n" + currentCourse.toString());
		
	}

}
